generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Affiliate {
  id        Int      @id @default(autoincrement())
  guildId   String
  userId    String
  link      String   @unique
  total     Int      @default(0)
  createdAt DateTime @default(now())
}
